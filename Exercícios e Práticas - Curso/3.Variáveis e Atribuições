#include <stdio.h>

#define texto "exemplo de texto para testar constante"

int main () {

  printf("Conte√∫do da constante: %s\n",texto);
 
  int idade = 0;
  float altura = 0.0;
  char nome[50] = "";
  
  printf("Digite a idade: ");
  scanf("%d", &idade);
  
  printf("Digite a altura: ");
  scanf("%f", &altura);
  
  printf("Digite o nome: ");
  scanf("%s",&nome);
  
  printf("Idade: %d\n",idade);
  printf("Altura: %.2f\n",altura);
  printf("Nome: %s\n",nome);
    
    
}
