//Soma dois números 

#include <stdio.h>

int main() {
    int num1, num2, soma;

    // Entrada de dados
    printf("Digite o primeiro número: ");
    scanf("%d", &num1);

    printf("Digite o segundo número: ");
    scanf("%d", &num2);

    // Processamento
    soma = num1 + num2;

    // Saída de dados
    printf("A soma de %d e %d é %d.\n", num1, num2, soma);

    return 0;
}
